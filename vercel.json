{
  "buildCommand": null,
  "outputDirectory": ".",
  "cleanUrls": true,
  "trailingSlash": false,
  "functions": {
    "api/upload.js": {
      "includeFiles": "logo-watermark.png"
    },
    "api/watermark.js": {
      "includeFiles": "logo-watermark.png",
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/view/:id",
      "destination": "/view.html?id=:id"
    }
  ]
}
